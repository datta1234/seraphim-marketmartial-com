    "jquery": "^3.2",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "laravel-mix": "^1.0",
    "lodash": "^4.17.4",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "nock": "^9.4.2",
    "popper.js": "^1.12",
    "sinon": "^6.0.0",
    "vue": "^2.5.7"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.11",
    "laravel-echo": "^1.4.0",
